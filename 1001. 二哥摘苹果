    #include<stdio.h>
    	int main(){
    		int high,dhigh,n;
    		scanf("%d %d %d",&high,&dhigh,&n);
    		int apple[n];
    		int cnt = 0;
    		for (int i = 0;i < n;i++){
    			scanf("%d",&apple[i]);
    			if (apple[i] <= high + dhigh){
    				cnt++;
    			}
    		}
    		printf("%d",cnt);
    		return 0;
    	}
